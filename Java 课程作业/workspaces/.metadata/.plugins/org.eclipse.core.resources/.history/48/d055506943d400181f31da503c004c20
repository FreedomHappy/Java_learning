abstract class Stuff{
	abstract public String toString();
}
class Book extends Stuff{
	String bookName;
	String press;
	@Override
	public String toString() {
		return bookName+'('+press+')';
	}
}

class Cloth extends Stuff{
	String brand;
	String color;
}

